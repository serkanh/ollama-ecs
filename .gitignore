*_override.tf.json

# Ignore CLI configuration files
.terraformrc
terraform.rc

# Ignore lock files
.terraform.lock.hcl

# Ignore plan output files
tfplan
*.tfplan

# Ignore .DS_Store files on macOS
.DS_Store

# Ignore .env files which might contain sensitive environment variables
.env

terraform.tfvars